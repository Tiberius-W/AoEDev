<!-- Color Vals -->
<Civ4ColorVals xmlns="x-schema:CB_CIV4InterfaceSchema.xml">
	<ColorVals>
		<ColorVal><!--general positive effect text-->
			<Type>COLOR_POSITIVE_TEXT</Type>
			<fRed>0.50</fRed>
			<fGreen>1.00</fGreen>
			<fBlue>0.10</fBlue>
			<fAlpha>1.00</fAlpha>
		</ColorVal>
		<ColorVal> <!--general negative effect text-->
			<Type>COLOR_NEGATIVE_TEXT</Type>
			<fRed>0.80</fRed>
			<fGreen>0.80</fGreen>
			<fBlue>1.00</fBlue>
			<fAlpha>1.00</fAlpha>
		</ColorVal>
		<ColorVal> <!--warning text ( used in prereqs, constraints)-->
			<Type>COLOR_WARNING_TEXT</Type>
			<fRed>0.80</fRed>
			<fGreen>0.80</fGreen>
			<fBlue>1.00</fBlue>
			<fAlpha>1.00</fAlpha>
		</ColorVal>
		<ColorVal>
			<Type>COLOR_ALIGNMENT_EVIL</Type>
		<fRed>0.80</fRed>
			<fGreen>0.80</fGreen>
			<fBlue>1.00</fBlue>
			<fAlpha>1.00</fAlpha>
			</ColorVal>
		
		<ColorVal> <!--text originally in red-->
			<Type>COLOR_RED</Type>
			<fRed>0.80</fRed>
			<fGreen>0.80</fGreen>
			<fBlue>1.00</fBlue>
			<fAlpha>1.00</fAlpha>
		</ColorVal>
		
	</ColorVals>
</Civ4ColorVals>
